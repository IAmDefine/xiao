page{background-color: #F5F5F5;}
.page{border-top: 1rpx solid #EAEAEA; color:#333333;font-size: 28rpx; font-family: '微软雅黑';}
.page view{box-sizing: border-box;}
.changeNameWrap{width: 100%;height: 90rpx;border-top: 1rpx solid #EAEAEA;border-bottom:1rpx solid #EAEAEA;background-color: #FFFFFF;margin: 20rpx 0 50rpx 0;padding: 0 0 0 20rpx;display: flex;}
.changeNameWrap>input{height: 90rpx;font-size: 28rpx;color: #333333;flex: 1;}
.changeNameWrap>view{width: 98rpx;height: 90rpx;display: flex;align-items: center;justify-content: center;}
.changeNameWrap>view image{width: 35rpx;height: 35rpx;}
.subBtnWrap{width: 100%;padding: 0 30rpx;}
.subBtnWrap view{width: 100%;height: 88rpx;text-align: center;line-height: 88rpx;color: #FFFFFF;border-radius: 10rpx;background-color: #DC3B32;}
























