page{background-color: #F5F5F5;}
.page{border-top: 1rpx solid #EAEAEA;font-family: '微软雅黑';color:#333333;font-size: 28rpx;}
.page view{box-sizing: border-box;}
.infoPicWrap{width: 100;padding: 65rpx 30rpx;background-color: #F3394A;margin: 20rpx 0;}
.infoPicWrap>image{width: 120rpx;height: 120rpx;float: left;margin-right: 24rpx;border-radius: 50%;}
.infoPicWrap>text{font-size: 30rpx;color: #FFFFFF;line-height: 120rpx;}
.selectChoiceWrap{width: 100%;background-color: #FFFFFF;margin-bottom: 20rpx;}
.selectChoiceItem{
  width: 100%;background-color: #FFFFFF;
}
.selectChoiceInWrap{width: 100%;padding-left: 30rpx;}
.selectChoice{width: 100%;padding-left: 54rpx;position: relative;}
.selectChoiceIcon{width: 54rpx;height: 92rpx;position: absolute;left: 0;top: 0;display: flex;align-items: center;}
.myInfoIcon1{width: 27rpx;height: 31rpx;}
.selectChoiceCon{width: 100%;height: 92rpx;line-height: 92rpx;border-bottom: 1rpx solid #EAEAEA;position: relative;}
.contactBtn{width: 100%;height: 100%;position: absolute;left: 0;top: 0;opacity: 0;}
.myInfoIcon2{width: 30rpx;height: 26rpx;}
.myInfoIcon3{width: 30rpx;height: 30rpx;}
.infoNewsTips{border-bottom:0;position: relative;}
.selectChoiceCon >text{float: left;}
.selectChoiceCon>image{width: 12rpx;height: 20rpx;float: right;margin: 35rpx 30rpx 0 0;}
.infoNewsIcon{width: 28rpx;height: 28rpx;border-radius: 50%;background-color: #EB2000;color: #FFFFFF;font-size: 20rpx;position: absolute;top: 31rpx;right: 62rpx;text-align: center;line-height: 28rpx;}
.myInfoIcon4{width: 30rpx;height: 28rpx;}

















