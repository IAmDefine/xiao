<view class='page'>
  <view class='setWrap'>
    <view class='setItem clear' bindtap='phone'>
      <text>手机账号</text>
      <image src='/images/myInfoIcon_right.png' wx:if="{{!mobile}}"></image>
      <text class='phone'>{{mobile}}</text>
    </view>
    <view class='setItem clear'  bindtap='tel'>
      <text>客服电话</text>
      <text class='phone'>010-85952988</text>
    </view>
    <view class='setItem mbn clear' bindtap='we'>
      <text>关于优星库</text>
      <image src='/images/myInfoIcon_right.png'></image>
    </view>
  </view>
</view>