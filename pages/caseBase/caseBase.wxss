page{background-color: #F5F5F5;}
.page{border-top: 1rpx solid #EAEAEA; color:#333333;font-size: 28rpx; font-family: '微软雅黑';}
.page view{box-sizing: border-box;}
/* 案例 */
.caseWrap{width: 100%;padding: 0 20rpx 50rpx 20rpx;}
.case{width: 100%;height: 420rpx;margin-top: 20rpx;position: relative;border-radius: 10rpx;overflow: hidden;background-color: #FFFFFF;}
.baseCasePic{ width: 100%;height: 420rpx; background-size: 100%; position: absolute;left: 0;top: 0;z-index: 1;}
.caseMask{width: 100%;height: 100%;position: absolute;left: 0;top: 0;z-index: 2;}
.caseMaskCheckWrap{width: 100%;height: 80rpx;position: absolute;left: 0;top: 0;}
.caseMaskCheckWrapMask{width: 100%;height: 80rpx;opacity: .5;background: linear-gradient(to bottom, #000000 ,transparent);position: absolute;left: 0;top: 0;}
.caseMaskCheck{position: absolute;top: 10rpx;right: 20rpx; display: flex;align-items: center;}
.caseMaskCheck>image{width: 24rpx;height: 17rpx;margin-right: 10rpx;float: left;}
.caseMaskCheck>text{float: left;color: #FFFFFF;font-size: 22rpx;}
.caseConWrap{width: 100%;height: 110rpx;position: absolute;left: 0;bottom: 0;}
.caseConMask{width: 100%;height: 110rpx;opacity: .5;background: linear-gradient(to top, #000000 ,transparent);position: absolute;left: 0;bottom: 0;}
.caseCon{color: #FFFFFF;width: 100%;padding-left:20rpx;position: absolute;left: 0;bottom: 20rpx;}




























