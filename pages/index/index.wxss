/*顶部搜索  */
.topbar{width: 710rpx;height: 64rpx;padding: 20rpx;border-top: 1rpx solid #DBDBDB;position: relative;}
.search_link{width: 513rpx;height: 62rpx;border: 1rpx solid #EAEAEA;float: left;border-radius: 10rpx;background-color: #ffffff;padding-left: 55rpx;position: relative;color: #B0B0B0;font-size: 26rpx;line-height: 62rpx;}
.search_link_icon{display: block;width: 24rpx;height: 24rpx;position: absolute;top: 19rpx;left: 21rpx;}
.filter{width: 118rpx; height: 62rpx;border: 1rpx solid #EAEAEA;text-align: center;line-height: 62rpx;border-radius: 10rpx;font-size: 26rpx;color: #333333;background-color: #ffffff;position: absolute;right: 20rpx;}

.img_con{
  box-sizing: border-box;
  padding: 0 20rpx;
  width: 750rpx;
}
.starcon{
  width: 100%;
  height: 980rpx;
  margin-bottom: 20rpx;
  box-shadow:0 10rpx 30rpx rgba(0,0,0,0.2);
}
.starcon >image{
  width:100%;
  height: 100%;
}
