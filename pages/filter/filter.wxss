page{padding-bottom: 108rpx;background-color: #F5F5F5;}
.weui-cell {
padding:15px 15px;
position:relative;
display:-webkit-box;
display:-webkit-flex;
display:flex;
-webkit-box-align:center;
-webkit-align-items:center;
align-items:center;
}

.filter_btn_container {
  width: 100%;
  height: 88rpx;
  font-size: 28rpx;
  overflow: hidden;
  position: fixed;
  left: 0;
  bottom: 0;
}
.reset_btn {
 width: 50%;
    height: 88rpx;
    border-top: 1rpx solid #EAEAEA;
    float: left;
    line-height: 88rpx;
    text-align: center;
    background-color: #FFFFFF;
}

.filter_btn {
   width: 50%;
    height: 88rpx;
    border-top: 1rpx solid #EAEAEA;
    float: left;
    line-height: 88rpx;
    text-align: center;
    background-color: #DC3B32;
    color: #FFFFFF;
}
.weui-select{height: 90rpx;line-height: 90rpx;color: #333333;font-size: 28rpx;margin-bottom: 0;padding-right: 0;}
.styleBorder{margin-top: 20rpx;border-top: 1rpx solid #EAEAEA;border-bottom: 1rpx solid #EAEAEA;border-right: 0;}
.weui-cells::before{border-top:0;}
.weui-cells::after{border: 0;}
.bob{border-bottom: 1rpx solid #EAEAEA;box-sizing: border-box;height: 90rpx;}
.weui-cell{padding: 0;padding-left: 30rpx; height: 90rpx;color: #333333;}
.weui-cell::before{border-color: #EAEAEA;}
.bort{border-top: 1rpx solid #EAEAEA;}
.bigbob{border-bottom: 1rpx solid #EAEAEA;}
.weui-select::before {border: 0;}
.weui-cell__ft_in-access::after {border: 0;}
.filterCle>text{float: left;}
.filterCle>image{float: right;margin:35rpx 30rpx 0 0;width: 12rpx;height: 20rpx; }
.filterCle >.filterClePic{margin-top: 0;}