<view class="page">
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell__bd">
      <picker bindchange="protype" range="{{pro_type}}">
        <view class="weui-select">产品类型：{{pro_type[pro_type_detail]}}</view>
      </picker>
    </view>
  </view>

  <view class="weui-cells weui-cells_after-title" style='margin-top:10px'>
    <view class="weui-cell__bd">
      <picker bindchange="sex" range="{{sex}}">
        <view class="weui-select">性别：{{sex[sex_detail]}}</view>
      </picker>
    </view>
  </view>
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell__bd">
      <picker bindchange="age" range="{{age}}">
        <view class="weui-select">年龄：{{age[age_detail]}}</view>
      </picker>
    </view>
  </view>
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell__bd">
      <picker bindchange="family" range="{{family}}">
        <view class="weui-select">家庭身份：{{family[family_detail]}}</view>
      </picker>
    </view>
  </view>


  <view class="weui-cells weui-cells_after-title" style='margin-top:10px'>
    <view class="weui-cell__bd">
      <picker bindchange="area" range="{{area_v}}">
        <view class="weui-select">艺人地区：{{area_v[area_detail]}}</view>
      </picker>
    </view>
  </view>

  <view class="weui-cells weui-cells_after-title" style='margin-top:10px'>
    <view class="weui-cell weui-cell_access" bindtap='click_industry'>
      <view class="weui-cell__hd">
        <view>适用行业</view>
      </view>
      <view class="weui-cell__bd"></view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{show_value}}</view>
    </view>

    <view class="weui-cell weui-cell_access" bindtap='select_profession'>
      <view class="weui-cell__bd">艺人职业</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{select_profession_value}}</view>
    </view>
    <view class="weui-cell weui-cell_access" bindtap='tag'>
      <view class="weui-cell__bd">艺人特质</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{tag_value}}</view>
    </view>
  </view>

  <view class="filter_btn_container">
    <view class="reset_btn" bindtap="reser_btn">重置</view>
    <view class="filter_btn" bindtap="up">确定</view>
  </view>
</view>