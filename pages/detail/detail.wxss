page{background-color: #F5F5F5;padding-bottom: 138rpx;}
.page{border-top: 1rpx solid #EAEAEA; color:#333333;font-size: 28rpx; font-family: '微软雅黑';}
.page view{box-sizing: border-box;}
/* 滑动轮播 */
.swiperWrap{width: 100%;overflow: hidden;position: relative;}
swiper{width:100%; height: 900rpx;}
.swiperPic{width: 100%;height: 100%;}
.swiperIndexWrap{width: 70rpx;height: 70rpx;border-radius: 50%;background-color: rgba(0,0,0,.8);position: absolute;bottom: 30rpx;right: 30rpx;color: #FFFFFF;text-align: center;line-height: 70rpx;}
.dyIndex{font-size: 32rpx;}
/* 艺人介绍 */
.introWrap{width: 100%;padding: 30rpx 30rpx 30rpx 20rpx;background-color: #FFFFFF;}
.introitem{width: 100%;}
.introitemPic{float: left;line-height: 80rpx;}
.introitemPic>image{width: 80rpx;height: 80rpx;float: left;border-radius: 50%;margin-right: 20rpx;}
.introitemCollectWrap{float: right;font-size: 24rpx;line-height: 80rpx;display: flex;align-items: center;}
.introitemCollectWrap>image{width: 66rpx;height: 66rpx;margin-right: 16rpx;}
.introLabel{width: 100%;}
.introLabel>view{padding: 4rpx 23rpx;background-color: #F5F5F5;text-align: center;font-size: 22rpx;float: left;border-radius: 50rpx;margin:30rpx 0 0 20rpx;}
.introLabel>view:nth-of-type(1){margin-left: 0;}
/* 基本信息 */
.basicInfo{width: 100%;color: #555555;}
.basicInfoTitle{width: 100%;padding: 0 20rpx;height: 90rpx;line-height: 90rpx;}
.basicInfoCon{width: 100%;}
.basicInfoItem{width:100%;font-size: 26rpx;border-bottom: 1rpx solid #EAEAEA;}
.tableMid{width: 50%;float: left;padding-left: 20rpx;background-color: #FFFFFF;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.basicInfoItem>view:nth-of-type(1){border-right: 1rpx solid #EAEAEA;}
.basicInfoItem>.tableMid{height:76rpx;line-height: 76rpx;}
.basicInfoItemAll{width: 100%;font-size: 26rpx;border-bottom: 1rpx solid #EAEAEA;background-color: #FFFFFF;padding-left: 150rpx;position: relative;}
.tableAll{height: 76rpx;line-height: 76rpx;position: absolute;left: 20rpx;top: 0;}
.tableAllCon{padding: 18rpx 0;color: #555555;padding-right: 20rpx;}
.clearBLine{border-bottom: 0;}
/* 照片视频 */
.photoVideo{width: 100%;}
.movieWrap{width: 100%;margin-bottom: 20rpx;}
.movieCaption{width: 100%;height: 88rpx;padding: 0 20rpx;background-color: #FFFFFF;border-bottom: 1rpx solid #EAEAEA;line-height: 88rpx;color: #555555;}
.detailMovie1{width: 28rpx;height: 28rpx;}
.movieCaptionIcon{float: left; height: 88rpx;display: flex;align-items: center;}
.movieCaptionIcon>image{float: left;margin-right: 20rpx;}
.movieCaptionLink{float: right;height: 88rpx;font-size: 24rpx;display: flex;align-items: center;}
.movieCaptionLink>image{width: 12rpx;height: 20rpx;margin-left: 10rpx;}
.movieCon{width: 100%;background-color: #FFFFFF;padding-right: 6rpx;}
.movieConItem{width: 50%;float: left;padding-left: 6rpx;padding-top: 6rpx;}
.movieCon{width: 100%;}
.movieCon>image{width: 100%;height: 440rpx;display: block;}
.movieConText{width: 100%;text-align: center;height: 80rpx;line-height: 80rpx;color: #555555;font-size: 26rpx;text-overflow: ellipsis;white-space: nowrap;overflow: hidden;}
.detailMovie2{width: 30rpx;height: 30rpx;}
.detailMovie3{width: 30rpx;height: 36rpx;}
.detailMovie4{width: 29rpx;height: 28rpx;}
/* 演绎经历 */
.opusWrap{width: 100%; padding: 0 20rpx;background-color: #FFFFFF;}
.opusItem{width: 100%;height: 440rpx;padding: 20rpx 0 0 330rpx;border-bottom: 1rpx solid #EAEAEA;position: relative;}
.opusItemPic{width: 300rpx;height: 400rpx;margin-right: 30rpx;position: absolute;left: 0;top: 20rpx;}
.opusItemPic>image{width: 100%;height: 100%;}
.opusList{width: 100%;font-size: 26rpx; color: #555555;margin-bottom: 9rpx;}
.opusItem:nth-last-child(1){border:0;}
.detailMovie5{width: 30rpx;height: 32rpx;}
.detailMovie6{width: 29rpx;height: 29rpx;}
/* 全部商务 */
.business{width: 100%;background-color: #FFFFFF;}
.businessItem{width: 100%;border-bottom: 1rpx solid #EAEAEA;display: flex;}
.businessList{width: 170rpx;border-right: 1rpx solid #EAEAEA;display: flex;align-items: center;justify-content: center;padding: 18rpx 0;}
.businessItem>view:nth-last-of-type(1){border-right: 0;}
.businessCon{flex: 1;text-align: center;line-height: 80rpx;}
.businessDetail{flex: 1;padding:18rpx 20rpx;}
.businessConItem{width: 100%;border-bottom: 1rpx solid #EAEAEA;display: flex;color: #555555;}
.clearMb{margin-bottom: 0;}
.clearBb{border-bottom: 0;}
/* 评价 */
.evalWrap{width: 100%;}
.evalItem{width: 100%;padding: 20rpx;background-color: #FFFFFF;margin-bottom: 20rpx;}
.evalItem:nth-last-of-type(1){margin-bottom: 0;}
.evalPic{width: 100%;line-height: 80rpx;margin-bottom: 30rpx;}
.evalPic>image{width: 80rpx;height: 80rpx;margin-right: 20rpx;float: left;border-radius: 50%;}
.evalIcon{width: 100%;margin-bottom: 30rpx;}
.evalIcon>image{width: 24rpx;height: 22rpx;float: left;margin-right: 10rpx;}
.evalCon{color: #555555; font-size: 26rpx;margin-bottom: 30rpx;}
.evalImg{width: 100%;margin-bottom: 30rpx;}
.evalImg>image{width: 210rpx;height: 210rpx;float: left;margin:0 10rpx 20rpx 0;}
.evalTime{width: 100%;font-size: 24rpx;color: #999999;}
.evalTime>text{float: left; margin-right:20rpx; }
/* 签约按钮 */
.subBtn{width: 100%;height: 88rpx;background-color: #DC3B31;color: #FFFFFF;text-align: center;line-height: 88rpx;position: fixed;left: 0;bottom: 0;}